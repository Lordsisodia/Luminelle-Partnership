{
  "name": "lumelle-partnership",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run validate:blog-content && tsc -b && vite build",
    "lint": "eslint src",
    "lint:tokens": "node scripts/check-legacy-brand-colors.mjs",
    "report:colors": "node scripts/report-raw-color-usage.mjs",
    "report:colors:contexts": "node scripts/report-color-contexts.mjs",
    "validate:tokens": "node scripts/validate-design-tokens.mjs",
    "validate:blog-content": "node scripts/validate-blog-content.mjs",
    "validate:boundaries": "node scripts/check-import-boundaries.mjs",
    "playwright:install": "playwright install chromium",
    "preview": "vite preview",
    "clerk:grant-admin": "node scripts/clerk-grant-admin.mjs",
    "upload:images": "node scripts/upload-images-to-supabase.mjs",
    "snapshots:landing": "node scripts/capture-landing-screenshots.mjs",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "litellm:proxy": "./scripts/start-litellm-proxy.sh",
    "litellm:proxy:start": "./scripts/start-litellm-proxy.sh start",
    "litellm:proxy:stop": "./scripts/start-litellm-proxy.sh stop",
    "litellm:proxy:status": "./scripts/start-litellm-proxy.sh status",
    "litellm:config": "node scripts/update-litellm-config.mjs",
    "agent:hub": "node --import=tsx src/agents/index.ts",
    "agent:cli": "node --import=tsx -e \"import { runAgentCLI } from './src/agents/index.ts'; runAgentCLI();\"",
    "agent:demo": "node scripts/demo-orchestrator.mjs",
    "agent:test": "node scripts/test-agent-hub.mjs"
  },
  "dependencies": {
    "@clerk/clerk-react": "^4.32.5",
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.22.0",
    "@hookform/resolvers": "^3.9.0",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/polaris": "^12.0.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@supabase/supabase-js": "^2.83.0",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/node": "^2.3.0",
    "@vercel/og": "^0.8.5",
    "@vercel/routing-utils": "^4.0.0",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.548.0",
    "markdown-to-jsx": "^9.3.5",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "posthog-js": "^1.306.1",
    "prompt-sync": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.28.0",
    "svix": "^1.82.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "8.6.15",
    "@storybook/addon-essentials": "8.6.15",
    "@storybook/addon-interactions": "8.6.15",
    "@storybook/react-vite": "8.6.15",
    "@storybook/test": "8.6.15",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/markdown-to-jsx": "^7.0.0",
    "@types/node": "^24.10.2",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.27.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "penthouse": "^2.3.3",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "storybook": "8.6.15",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vercel": "^25.2.0",
    "vite": "^6.0.7",
    "vite-plugin-pwa": "^1.2.0"
  }
}
